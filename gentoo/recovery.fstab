/recovery           emmc		/dev/block/platform/soc/sdc2/by-name/recovery	flags=display="Recovery";backup=1;flashimg=1;
/misc               emmc		/dev/block/platform/soc/sdc2/by-name/misc    	flags=display="Misc";backup=1;flashimg=1;
/system             ext4		/dev/block/platform/soc/sdc2/by-name/system  	flags=display="System";backup=1;
/system_image       emmc		/dev/block/platform/soc/sdc2/by-name/system  	flags=display="System image";backup=1;flashimg=1;
/vendor             ext4		/dev/block/platform/soc/sdc2/by-name/vendor  	flags=display="Vendor";backup=1;
/vendor_image       emmc		/dev/block/platform/soc/sdc2/by-name/vendor  	flags=display="Vendor image";backup=1;flashimg=1;
/cache              ext4		/dev/block/platform/soc/sdc2/by-name/cache   	flags=display="Cache";
